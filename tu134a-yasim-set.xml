<?xml version="1.0" encoding="UTF-8"?>

<PropertyList include="tu134a-common.xml">
  <sim>
    <description>Tu-134A (YASim)</description>
    <author>New Version: lastmin-II, xcvb; Original Tu-134: Emmanuel BARANGER, Artem Kovalchuk and Gary Buckaroo; some stuff from other models: Yurik V. Nikiforoff, specter</author>

    <rating>
      <FDM type="int">     4 </FDM>
      <systems type="int"> 0 </systems>
      <cockpit type="int"> 3 </cockpit>
      <model type="int">   5 </model>
    </rating>

    <flight-model>yasim</flight-model>
    <aero>tu134a-yasim</aero>
  </sim>

 <input>
  <keyboard>
   <key n="127">
    <name>Delete</name>
    <desc>Toggle Reversers</desc>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[0]/reverser</property>
    </binding>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[1]/reverser</property>
    </binding>
   </key>
  </keyboard>
 </input>
</PropertyList>
