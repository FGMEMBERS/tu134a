<PropertyList>
  <path>agb.ac</path>
  <!--External panel lighting-->
  
  <animation>
    <type>material</type>
    <object-name>PitchScale</object-name>
    <object-name>Blanker</object-name>
    <object-name>Plane</object-name>
    <object-name>Plane.001</object-name>
    <object-name>Cylinder</object-name>
    <emission>
      <factor-prop>Tu-134/instrument-lights</factor-prop>
      <red>   1.0 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>

<animation>
    <type>noshadow</type>
    <object-name>glass</object-name>
  </animation>
  
  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/tu134a/Models/Effects/glass/glass_shader.png</texture>
    <object-name>glass</object-name>
  </animation>

  <!--Roll-->
  <animation>
    <type>textranslate</type>
    <object-name>PitchScale</object-name>
    <property>/instrumentation/attitude-indicator[1]/indicated-pitch-deg</property>
    <interpolation>
      <entry><ind>	-80.0</ind>	<dep>	-0.437</dep></entry>
      <entry><ind>	-70.0</ind>	<dep>	-0.383</dep></entry>
      <entry><ind>	-60.0</ind>	<dep>	-0.327</dep></entry>
      <entry><ind>	-50.0</ind>	<dep>	-0.273</dep></entry>
      <entry><ind>	-40.0</ind>	<dep>	-0.218</dep></entry>
      <entry><ind>	-30.0</ind>	<dep>	-0.163</dep></entry>
      <entry><ind>	-20.0</ind>	<dep>	-0.109</dep></entry>
      <entry><ind>	-10.0</ind>	<dep>	-0.054</dep></entry>
      <entry><ind>	-5.0</ind>	<dep>	-0.027</dep></entry>
      <entry><ind>	0.0</ind>	<dep>	0.0</dep></entry>
      <entry><ind>	5.0</ind>	<dep>	0.026</dep></entry>
      <entry><ind>	10.0</ind>	<dep>	0.053</dep></entry>
      <entry><ind>	20.0</ind>	<dep>	0.108</dep></entry>
      <entry><ind>	30.0</ind>	<dep>	0.163</dep></entry>
      <entry><ind>	40.0</ind>	<dep>	0.218</dep></entry>
      <entry><ind>	50.0</ind>	<dep>	0.273</dep></entry>
      <entry><ind>	60.0</ind>	<dep>	0.327</dep></entry>
      <entry><ind>	70.0</ind>	<dep>	0.382</dep></entry>
      <entry><ind>	80.0</ind>	<dep>	0.437</dep></entry>
    </interpolation>
    <!--<factor>0.0053</factor>-->
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  

<!--Attitude-->
<animation>
  <type>rotate</type>
  <object-name>Plane</object-name>
  <property>/instrumentation/attitude-indicator[1]/indicated-roll-deg</property>
  <max-deg>70.0</max-deg>
  <min-deg>-70.0</min-deg>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>

<!--Slip-->
<animation>
  <type>rotate</type>
  <object-name>slip_ball</object-name>
  <property>/instrumentation/slip-skid-ball/indicated-slip-skid</property>
  <max-deg>10.0</max-deg>
  <min-deg>-10.0</min-deg>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.006</z-m>
  </center>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>



</PropertyList>
