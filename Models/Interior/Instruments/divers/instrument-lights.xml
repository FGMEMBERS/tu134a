<?xml version="1.0" encoding="UTF-8"?> 

<PropertyList>

  <path>switch.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>Switch</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>base</object-name>
    <object-name>Switch</object-name>
    <emission>
      <red>   1.0 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>base</object-name>
    <object-name>Switch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>Tu-134/instrument-lights</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Switch</object-name>
    <property>Tu-134/instrument-lights</property>
    <factor> 35 </factor>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
